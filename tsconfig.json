{
  "compilerOptions": {
    "target": "ESNext", 
    "module": "ESNext",  
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",                             
    "rootDir": "./src",  
    "moduleResolution": "Node",
    "strictNullChecks": true,
    "resolveJsonModule": true
  },
  "exclude":[
    "./node_modules",
    "./default"
  ]
}
